    "scripts": {
        "cat-env": "cat ./src/environments/environment.ts",
        "config": "ts-node set-env.ts",
        "start": "npm run config && node server",
        "serve": "npm run config && ng serve --port $PORT",
        "build": "npm run config && ng build",
        "build-prod": "npm run config && ng build --prod",
        "test": "npm run config && ng test --code-coverage --watch=true",
